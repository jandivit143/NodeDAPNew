<doctype html>
    <html>
        <head>
            <title><%- title %></title>
        </head>
        <body>
            <h1 style="text-align: center;"><%- title %></h1>
            <ul style="list-style-type: none;">
                <% recipeList.forEach(function(recipe){ %>
                    <li>
                        <%- recipe.id %>&nbsp;<a href="<%- recipe.url %>"> <%- recipe.name %> </a>
                    </li>
                <% }); %>
            </ul>
        </body>
    </html>
</doctype>